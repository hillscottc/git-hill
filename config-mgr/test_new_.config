    <add connectionString="Data Source=usfshwssql104;Initial Catalog=RDxETL;Integrated Security=True" name="UMG.RDx.ETL.MP.Transform.Properties.Settings.RDxETLConnectionString" providerName="System.Data.SqlClient" />

    <add connectionString="Data Source=usfshwssql104\RIGHTSDEV_2;Initial Catalog=RDxReport;Integrated Security=True" name="UMG.RDx.ETL.MP.Load.Properties.Settings.RDxReportConnectionString" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <system.serviceModel>
